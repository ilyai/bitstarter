              <li class="active"><a href="/contact">Contact</a></li>
            </ul>
          </div><!--/.nav-collapse -->
        </div>
      </div>
    </div>
    <div class="container">
      <h2>Contact</h2>
      <form method="post">
        <fieldset>
          <label>Name</label>
          <input type="text" name="username" class="span3" placeholder="John Doe" required>
          <label>Message</label>
          <textarea name="message" class="input-block-level span5" rows="10" placeholder="Hello!" required></textarea>
          <button type="submit" class="btn btn-primary">Send</button>
        </fieldset>
      </form>
    </div>
  </body>
